<mjml>
  <mj-head>
    <mj-title>Publication Request: {{package_title}}</mj-title>
    <mj-preview>Publication Request: {{package_title}}, by author {{user_name}}</mj-preview>
    <mj-attributes>
      <mj-class name="title" font-size="16px" />
      <mj-all font-family="helvetica" font-size="14px" />
    </mj-attributes>
  </mj-head>

  <mj-body>
    <mj-section>

      <mj-column>

        <mj-image width="50px" alt="EnviDat Logo" src=" data:image/png;base64,"></mj-image>

        <mj-divider border-color="#007b83"></mj-divider>

        <mj-spacer />
        <mj-text>
          <mj-raw>
            {{#if is_update}}
              Update request for publication: 
            {{else}}
              Publication request for: 
            {{/if}}
          </mj-raw>
          <a href="{{package_url_prefix}}/{{package_title}}">{{package_title}}</a>
          by {{user_name}} ({{user_email}})
        </mj-text>

        <mj-spacer />
        <mj-button href="{{approval_url}}/approve?package_id={{package_title}}">Approve</mj-button>
        
        <mj-spacer />
        <mj-text>Have a nice day!</mj-text>
        <mj-text>EnviDat Support</mj-text>
        <mj-text>--</mj-text>
        <mj-text>Message sent by EnviDat ({{site_url}})</mj-text>

      </mj-column>

    </mj-section>

    <mj-section background-color="#007b83">
      <mj-column>
        <mj-social>
          <mj-social-element name="twitter" href="https://twitter.com/envidatservice" />
        </mj-social>
      </mj-column>
    </mj-section>
  </mj-body>
</mjml>
